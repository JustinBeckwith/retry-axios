{
  "name": "retry-axios",
  "version": "0.0.0",
  "description": "Retry HTTP requests with Axios.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JustinBeckwith/retry-axios.git"
  },
  "scripts": {
    "lint": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "compile": "tsc -p tsconfig.json --outDir build --allowSyntheticDefaultImports",
    "build-web": "microbundle --tsconfig=tsconfig.json",
    "umd": "rm -rf umd && babel build/src -d umd --source-maps",
    "test": "c8 mocha -r=esm build/test",
    "prepare": "npm run build-web",
    "pretest": "npm run compile",
    "license-check": "jsgl --local ."
  },
  "keywords": [
    "axios",
    "retry"
  ],
  "author": {
    "name": "Justin Beckwith"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "axios": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/mocha": "^7.0.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.0.4",
    "axios": "^0.19.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "c8": "^7.0.0",
    "codecov": "^3.5.0",
    "esm": "^3.2.25",
    "gts": "^2.0.0",
    "js-green-licenses": "^2.0.0",
    "json-schema": "^0.2.5",
    "microbundle": "^0.12.0",
    "mocha": "^7.0.0",
    "nock": "^12.0.0",
    "semantic-release": "^17.0.4",
    "typescript": "~3.9.0"
  },
  "files": [
    "dist"
  ],
  "c8": {
    "exclude": [
      "build/test"
    ]
  },
  "browserslist": "> 1%, last 2 versions, Firefox ESR"
}
